{
  "name": "BuTags",
  "description": "backup bookmarks and restore.",
  "version": "1.0",
  "permissions": [
    "tabs", "activeTab", "<all_urls>", "storage"
  ],
  "browser_action": {
      "default_title": "BuTags.",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
